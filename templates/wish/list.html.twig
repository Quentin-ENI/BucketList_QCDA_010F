{% extends 'base.html.twig' %}

{% block title %}All wishes{% endblock %}

{% block main %}
    <main>
        <h1>All wishes !!</h1>

        {% for wish in wishes %}
            <p><a href="{{ path('wish_details', { id: wish.id }) }}">{{ wish.title }}</a></p>
        {% endfor %}
    </main>
{% endblock %}

